        <template><![CDATA[<!-- Template created for: "MEUS EQUIPAMENTOS E APLICAÇÕES" -->
<div>
  <div>
    <div ng-if="data.showEmptyMessage" class="without_notification">
      ${Você ainda não tem equipamentos ou aplicações.}
    </div>
    <table ng-if="!data.showEmptyMessage" class="table table-hover b-t table-condensed cart-table assets_content">
      <thead>
        <tr>
          <th class="col-lg-2 wrapper column_content" ng-repeat="column in data.columns">
            <div ng-click="c.orderBy(column.order, data.assetsTab)">
              {{column.name}}
              <span ng-if="column.order">
                <span ng-if="!column.direction"><!--VAZIO--></span>
                <span ng-if="column.direction"><img src="gservice_column_dashboard_arrow.png" ng-class="column.direction == -1 ? 'invert-arrow' : ''"}/></span>
              </span>
            </div>
          </th>
        </tr>
      </thead>
      <tbody>
        <tr class="item-row b-b" ng-repeat="item in c.data.assets">
          <td class="column_name" data-th="Product">
            <!-- Necessário query para buscar se é equipamento ou aplicação -->
            <div class="asset_name">{{item.title}}</div>
          </td>
          <td>
            <div class="service_title">{{item.description}}</div>
          </td>
          <td>
            <div class="service_title">${GService View More}</div>
          </td>
          <td>
            <div ng-click="">
              <img src="GService_DASH_ReportarErro.png"/>
            </div>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>

<now-message key="Done" value="${Done}"/>]]></template>
    </sp_ng_template>
</record_update>
