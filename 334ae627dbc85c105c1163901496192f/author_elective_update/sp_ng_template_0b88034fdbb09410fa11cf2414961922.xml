        <template><![CDATA[<div class="content page_cat_item" id="show-item">

  <div class="header-loader" ng-show="c.isLoading">
    <div class="hidden-xs sp-loading-indicator" ng-if="loadingIndicator">
      <div></div>
      <div></div>
      <div></div>
    </div>
  </div>

  <div ng-show="!c.isLoading" class="sc_catalog_item">
    <div id="sc_cat_item" sn-atf-blacklist="IS_SERVICE_CATALOG">
      <div class="row" ng-if="data.sc_cat_item">
        <div class="col-sm-12" id="catItemTop">
          <!-- Info Message Box -->
          <div ng-if="data.show_wishlist_msg" class="alert alert-info">
            {{m.itemWishlistMsg}}
          </div>
          <br>

          <div>
            <div ng-class="{true: '', false: 'sc-sticky-item-header'}[!c.data.sc_cat_item.short_description]" style="top: {{stickyHeaderTop}}"> <!-- wrapper-md b-b break-word item-header -->
              <h1 class="h2 m-t-none m-b-sm font-thin title {{data.categoryColor}}" ng-if="data.sc_cat_item.name && options.native_mobile != 'true'">{{data.sc_cat_item.name}}</h1>
            </div>

            <div class="row b-b no-margin" ng-if="c.data.sc_cat_item.picture || c.data.sc_cat_item.description" ng-class="{'wrapper-md': options.native_mobile != 'true', 'wrapper-sm': options.native_mobile == 'true'}">
              <div class="col-sm-4 col-xs-12 no-padder" ng-if="c.data.sc_cat_item.picture">
                <div class="wrapper-md text-center">
                  <i class="fa fa-chevron-left pointer" style="position:absolute; top:50%; left:4%; color:#CECECE" ng-if="options.image_gallery"></i>
                  <img class="img-responsive catalog-item-image" alt="{{data.sc_cat_item.name}}" style="display: inline" ng-src="{{data.sc_cat_item.picture}}?t=medium" />
                  <i class="fa fa-chevron-right pointer" style="position:absolute; top:50%; right:4%; color:#CECECE" ng-if="options.image_gallery"></i>
                  <div class="image-gallery padding-top" ng-if="options.image_gallery">
                    <i class="fa fa-circle active"></i>
                    <i class="fa fa-circle"></i>
                    <i class="fa fa-circle"></i>
                  </div>
                </div>
              </div>
              <div class="col-xs-12 break-word"
                   ng-class="{true: 'col-sm-12 no-padder', false: 'col-sm-8'}[!c.data.sc_cat_item.picture]">

                <div class="col-xs-12 col-sm-12 visible-xs visible-sm" ng-if="c.options.show_less_description === 'true'">
                  <sc-toggle-data sn-data="data.sc_cat_item.description"></sc-toggle-data>
                </div>
              </div>
            </div>
            <div class="col-md-4" ng-class="{'b-b wrapper-md': !data.no_fields}">
              <sp-cat-item item="data.sc_cat_item" ></sp-cat-item>
              <div class="{{data.categoryColor}} form-control-default">
                <form>
                  <!-- display view and model -->
                  <sp-model form-model="data.sc_cat_item" mandatory="c.mandatory"></sp-model>
                </form>
              </div>

            </div>
            <div class="col-md-8 m-t-lg">
              <div class="visible-md visible-lg" ng-class="{false : 'visible-xs visible-sm', true : 'hidden-xs hidden-sm'}[c.options.show_less_description === 'true']">
                <div ng-bind-html="data.sc_cat_item.description" class="sc-item-description"></div>
              </div>

              <div class="content_short_description">
                <div class="shortdescription_title">${Informações Complementares}:</div>
                <div class="text-muted sc-cat-item-short-description" ng-if="data.sc_cat_item.short_description">{{data.sc_cat_item.short_description}}</div>
              </div>
            </div>

            <div class="col-md-12">

              <div class="col-md-7">
                <div>
                  <button tabindex="0" ng-if="data.table == 'sc_cat_item'" name="add_to_cart" ng-click="triggerAddToCart()" class="btn btn-primary btn-request-service {{data.categoryColor}}">
                    <img src="{{data.categorySubmitImg}}"/>
                    ${Request Service}
                  </button>
                  <button tabindex="0" ng-if="data.table == 'incident'" name="submit" ng-disabled="submitting || submitted" ng-click="triggerOnSubmit()" class="btn btn-primary btn-submit-incident">
                    <img src="GService_Reportar_Erro.png"/>
                    ${Submit}
                  </button>
                  <button tabindex="0" ng-if="data.is_cart_item" name="update" ng-disabled="submitting || submitted" ng-click="triggerUpdateCart()" class="btn btn-default">
                    <i class="fa fa fa-shopping-cart m-r-xs"></i>
                    ${Update Cart}
                  </button>

                  <button ng-if="c.showOrderNowButton()" tabindex="0" name="cancel" ng-disabled="submitting || submitted" ng-click="cancelItem()" class="btn btn-primary btn-cancel">
                    <img src="trash.png"/>
                    ${cancel}
                  </button>
                </div>

                <div ng-show="hasMandatory(c.mandatory)" class="alert alert-info" style="margin-top: .5em" ng-if="c.options.show_field_validation_messages === 'true'">
                  <div ng-if="hasMandatory(mandatory)">${Required information} </div>
                  <label ng-repeat="f in c.mandatory" class="label sc-field-error-label" style="margin-right: .5em; display: inline-block;" ng-click="getFocus(f)">{{::f.label}}</label>
                </div>
              </div>

              <!-- Attachments -->
              <div ng-if="c.showAttachments()" class="cold-md-5 wrapper-md row content-attachments">
                <div ng-class="{'flex-center attachment-height': options.native_mobile == 'true', 'flex-end': options.native_mobile != 'true'}"> <!-- -->
                  <label ng-if="!submitting" style="font-weight:normal;cursor:pointer;">
                    <sp-attachment-button></sp-attachment-button>
                    <span class="fa fa-asterisk mandatory" 
                          ng-if="data.sc_cat_item.mandatory_attachment" 
                          ng-class="{'mandatory-filled': data.sc_cat_item.mandatory_attachment && (data.sc_cat_item.attachment_submitted || attachments.length > 0)}"
                          style="vertical-align:super"></span>
                    <span class="attach_text {{data.categoryColor}}">${Add attachments}</span>
                  </label>
                </div>
                <now-attachments-list class="attachment-list" template="sp_attachment_single_line" ></now-attachments-list>
              </div>
            </div>
          </div>


        </div>
      </div>
    </div>


  </div>

</div>

<!-- Checkout Modal -->
<div class="anim_bg_checkout_modal">
  <div class="bg_checkout_modal" ng-click="c.closeCheckoutModal()" ng-if="data.ordered_item"></div>
</div>

<div class="checkout_modal_request" ng-if="data.ordered_item">
  <div class="modal_header">
    <div class="modal_title">${CHECKOUT}</div>
    <div class="close_modal globo_buttons" ng-click="c.closeCheckoutModal()">x</div>
  </div>
  <div class="modal_line"></div>
  <div class="modal_body">
    <div class="modal_body_fields">
      <div class="modal_breadcrumb">{{data.breadcrumb}}</div>
      <div class="modal_name_item">{{data.sc_cat_item.name}}</div>
      <div>${Requested for:} {{c.data.user.name}}</div>
      <div>${Email}: {{data.user.email}}</div>
      <div>${Location Portal}: {{data.user.location}}</div>
      <div>${Phone}: {{data.user.phone}}</div>
    </div>
    <div class="modal_body_buttons">
      <div class="globo_buttons" ng-click="c.closeCheckoutModal()"><img src="duplicate.png" /></div>
      <div class="globo_buttons" ng-click="c.removeItem()"><img src="trash.png" /></div>
    </div>
  </div>
  <div class="modal_line"></div>
  <div class="modal_footer">
    <div class="checkout_button globo_buttons" ng-click="">
      <div><img src="submit.png" /></div>
      <div class="text_submit" ng-click="triggerOnSubmit()">${Finalizar pedido}</div><!-- ng-click="c.goToCart()" -->
    </div>
    <div class="continue_shopping globo_buttons" ng-click="c.continueShopping()">${Continuar comprando}</div>
  </div>
</div>

<div class="checkout_modal_incident" ng-if="data.ordered_incident">
  <div class="close_modal" ng-click="c.closeIncidentCheckout()">x</div>
  <div class="modal_title">{{c.data.username}},<br>Sua solicitação foi enviada com sucesso!</div>
  <div class="modal_category">Você reportou um erro na categoria<br>{{c.data.incidentCat}}.</div>
  <div class="modal_summary">${Resumo da solicitação:} {{c.data.summary}}</div>
  <div>${Tipo de erro:} {{c.data.error}}</div>
  <div ng-if="c.data.affectedUsers">${Afetados:} {{c.data.affectedUsers}}</div>
  <div ng-if="c.data.impact">${Impacto:} {{c.data.impact}}</div>
  <div class="modal_deadline">Prazo de atendimento previsto: XX horas</div>
</div>

<now-message key="Added item to shopping cart" value="${Added item to shopping cart}"/>
<now-message key="Attachment(s) are not added" value="${Attachment(s) are not added}"/>
<now-message key="Leave page?" value="${Leave page?}"/>
<now-message key="Changes you made will be lost." value="${Changes you made will be lost.}"/>
<now-message key="Cancel" value="${Cancel}"/>
<now-message key="Leave" value="${Leave}"/>]]></template>
    </sp_ng_template>
